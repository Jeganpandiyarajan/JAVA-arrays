 public class Main
{
	public static void main(String[] args) {
		int arr[]={7,10,4,3,20,15},k=4;
		int n=arr.length;
		for(int i=0;i<n-1;i++){
		    for(int j=0;j<n-i-1;j++){
		        if(arr[j]>arr[j+1]){
		            int temp= arr[j];
		            arr[j]=arr[j+1];
		            arr[j+1]=temp;
		        }
		    }
		}
	   System.out.print(arr[k-1]);
	    
	}
}
