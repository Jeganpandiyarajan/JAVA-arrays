public class Main
{
	public static void main(String[] args) {
		int i=0;
		int count=0;
		int x=4;
		int arr[]={23,4,6,1,4,6,6,6};
		while(i<arr.length){
		    if(x==arr[i]){
		        count++;
		    }
		    i=i+1;
		}
		System.out.println(count);
		if(i==arr.length){
		    System.out.print("sry");
		}
	}
}
