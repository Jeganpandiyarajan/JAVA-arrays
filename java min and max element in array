public class Main
{
	public static void main(String[] args) {
		int arr[]={12,1234,45,67,1};
		int n=arr.length;
		//mim func
		int m=arr[0];
		for(int i=1;i<n;i++){
		    m=Math.min(m,arr[i]);
		    
		}
		System.out.println("min ele :"+m);
		// max func
		for(int i=1;i<n;i++){
		    m=Math.max(m,arr[i]);
		    
		}
		System.out.println("max ele :"+m);
	}
}
